{
  "version": 2,
  "alias": ["cssconf", "cssconf.co", "www.cssconf.co"],
  "routes": [
    {
      "src": "/(.*)",
      "status": 301,
      "headers": { "Location": "https://cssconf.co/$1" }
    }
  ],
  "env": {
    "GOOGLE_ANALYTICS_ID": "@cssconfco-google-analytics-id",
    "MAILCHIMP_API_KEY": "@cssconfco-mailchimp-api-key",
    "MAILCHIMP_AUDIENCE_ID": "@cssconfco-mailchimp-audience-id"
  }
}
